{
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "rules": {
    "react/prop-types": "off",
    "comma-dangle": "off",
    "class-methods-use-this": "off",
    "no-alert": "off",
    "no-return-assign": [
      "error",
      "except-parens"
    ],
    "no-console": "off",
    "jsx-a11y/no-static-element-interactions": "off"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
